{{#each members}}
<div class="cards">
<article class="card">
    <div class="details">
      <div class="prenom">{{prenom}}</div>
      <div class="dateOfBirth">{{dateOfBirth}}</div>
      <div class="email">{{email}}</div>
      <div class="message">{{message}}</div>      
      <div class="btn">
        <div class="editer">
          <p>
           <a class="btn2" href="/birthdates/{{id}}">Editer</a>
          </p>
        </div>
        <div class="supprimer">
        <form action="/birthdates/{{id}}/delete" method="POST">
            <button class="btn1">Supprimer</button>
         </form>
        </div>
      </div>
  </div>
</article>
</div>
{{/each}}

